{
  "name": "texlab",
  "displayName": "TexLab",
  "description": "LaTeX for Visual Studio Code",
  "version": "0.0.1",
  "publisher": "efoerster",
  "license": "MIT",
  "repository": "https://github.com/efoerster/texlab",
  "engines": {
    "vscode": "^1.18.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:latex",
    "onCommand:extension.sayHello"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.sayHello",
        "title": "Hello World"
      }
    ],
    "languages": [
      {
        "id": "latex",
        "aliases": [
          "LaTeX",
          "latex"
        ],
        "extensions": [
          ".tex",
          ".sty",
          ".cls"
        ],
        "configuration": "./latex-language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "latex",
        "scopeName": "source.latex",
        "path": "./syntaxes/latex.tmLanguage"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "lint": "tslint --project .",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^9.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "vscode-languageclient": "^3.5.0"
  }
}
