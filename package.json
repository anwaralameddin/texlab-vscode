{
  "name": "texlab",
  "displayName": "TexLab",
  "description": "LaTeX for Visual Studio Code",
  "version": "0.1.0",
  "publisher": "efoerster",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/efoerster/texlab-vscode"
  },
  "homepage": "https://github.com/efoerster/texlab-vscode/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/efoerster/texlab-vscode/issues"
  },
  "engines": {
    "vscode": "^1.25.1"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Other"
  ],
  "keywords": [
    "latex",
    "tex",
    "bibtex"
  ],
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#F5F5F5",
    "theme": "dark"
  },
  "preview": true,
  "activationEvents": [
    "onLanguage:latex",
    "onLanguage:bibtex"
  ],
  "main": "./out/main",
  "contributes": {
    "configuration": [
      {
        "title": "LaTeX configuration",
        "properties": {
          "latex.build.executable": {
            "type": "string",
            "default": "latexmk",
            "description": "Path to a LaTeX build tool."
          },
          "latex.build.args": {
            "type": "array",
            "default": [
              "-pdf",
              "-interaction=nonstopmode",
              "-synctex=1"
            ],
            "description": "Additional arguments that are passed to the build tool."
          }
        }
      }
    ],
    "commands": [
      {
        "command": "latex.build",
        "title": "LaTeX: Build File",
        "description": "Builds the current file."
      }
    ],
    "keybindings": [
      {
        "command": "latex.build",
        "key": "ctrl+f5",
        "mac": "cmd+f5"
      }
    ],
    "languages": [
      {
        "id": "latex",
        "aliases": [
          "LaTeX",
          "latex"
        ],
        "extensions": [
          ".tex",
          ".sty",
          ".cls"
        ],
        "configuration": "./languages/latex.json"
      },
      {
        "id": "bibtex",
        "aliases": [
          "BibTeX",
          "bibtex"
        ],
        "extensions": [
          ".bib"
        ],
        "configuration": "./languages/bibtex.json"
      }
    ],
    "grammars": [
      {
        "language": "latex",
        "scopeName": "source.latex",
        "path": "./grammars/latex.tmLanguage.json"
      },
      {
        "language": "bibtex",
        "scopeName": "source.bibtex",
        "path": "./grammars/bibtex.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "lint": "tslint --project .",
    "format": "prettier --write \"src/**/*.{ts,json}\" \"{grammars, languages}/**/*.json\" \"*.{json,md}\" "
  },
  "devDependencies": {
    "@types/node": "^10.5.2",
    "prettier": "^1.13.7",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "vscode-languageclient": "^4.3.0"
  }
}
